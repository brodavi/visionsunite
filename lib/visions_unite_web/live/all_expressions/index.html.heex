<NavComponent.nav active={@live_action} />

<br>

<%= if Enum.count(@all_expressions) != 0 do %>
  <section>
    <h3>ALL Expressions</h3>
    <div id="all-expressions">
      <%= for expression <- @all_expressions do %>
        <div id={"supported-expression-#{expression.id}"} class="roww">
          <div>
            <ExpressionComponent.expression expression={expression} current_user_id={@current_user_id} />
          </div>
        </div>
      <% end %>
    </div>
  </section>
<% end %>

